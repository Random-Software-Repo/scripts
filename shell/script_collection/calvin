#!/usr/bin/zsh

SLEEP=0.01

function 	slowprint()
{
	sleep $SLEEP
	print "${1}"
}
function frame00
{
	slowprint '                                                $   $  $   $'
	slowprint '                                                 "o  $ $  o""'
	slowprint '                                                   o  "   "ooooo'
	slowprint '                                               oo ""           o$'
	slowprint '                                 o            o            oo  "'
	slowprint '                                $$             $o$""$o  ooo$'
	slowprint '                                $"$          o $    "$  $'
	slowprint '                      o$o       $ "$         $ $     $ $'
	slowprint '                       $$$o     $$ "$       o$ $     o $o'
	slowprint '                       "$ ""o   "$   "o     $$ "o     o"'
	slowprint '                   $o   $$   "o  $     "oo  $"  $   o$"'
	slowprint '                    "$   $o    "o$$       "o$    $o$" oo$'
	slowprint '                      "o "$o                     "$o $"$$'
	slowprint '                        "        oo$$$$$$$oo        $oo$$""      o" o'
	slowprint '               """""""""      o$$$$$$$$$$$"$o             o"""$o$$  o$'
	slowprint '                     ooo$$$"o$$$$$$$$$$$$$$ "$o    o   o$$$o   $ $ o$'
	slowprint '                  o$$$$$$$$$$$$$$$$$$$$$$$$    "oo  o      ""o  "$ $'
	slowprint '                 $$$$$$$$$$$$$$$$$$$$$$$$$$      "$o   o$$"""$     " oo""o'
	slowprint '              o""""$$$$$$$$$$$$$$$$$$$$$$"         ""$o"$o          "   o$'
	slowprint '                   "$$$$$$$$""""$""$$$""              "$oo$""$o     o$"""'
	slowprint '                    $$$$$$$"                           $""""$"  o""""'
	slowprint '                     $"""""$ooooo        ooooo$$$$$$$     o$" o"'
	slowprint '                      $     """" oooo$$$$$$$$$$$$$$"     $"  o"'
	slowprint '                    oo$   oooo$$$$$$$$$$$"""""$$$$"    o$" o$"'
	slowprint '                  "$ $o$$$$$$$$$$$$$""$     o$$$"oooo$"  o"'
	slowprint '                    "o$ "$$$$$$$$$$$$         $$o$"$$$   $"'
	slowprint '                      "$  ""$$$$$$$$$        o$"$$$ "$$o$$'

	slowprint '                                     $ o$$o  $$o$$"          oo$$'
	slowprint '                                     $ $$$$  $$$$$$$$$$$$$$$$$$$$'
	slowprint '                                     $ $$$$  $$$$$$$$$$$$$$$$$$$$"'
